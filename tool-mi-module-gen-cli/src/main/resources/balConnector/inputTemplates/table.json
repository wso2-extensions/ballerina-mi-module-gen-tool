{
  "type": "table",
  "value": {
    "name": "{{name}}",
    "displayName": "{{displayName}}",
    "description": "{{description}}",
    "tableKey": "{{tableKey}}",
    "tableValue": "{{tableValue}}",{{#if enableCondition}}
    "enableCondition": {{{enableCondition}}},{{/if}}
    "elements": [
{{#each elements}}
      {
        "type": "attribute",
        "value": {
          "name": "{{name}}",
          "displayName": "{{displayName}}",
          "inputType": "{{inputType}}",
          "required": {{required}},
          "helpTip": "{{helpTip}}"{{#if validateType}},
          "validateType": "{{validateType}}"{{/if}}{{#if matchPattern}},
          "matchPattern": "{{matchPattern}}"{{/if}}
        }
      }{{#unless @last}},{{/unless}}
{{/each}}
    ]
  }
}
